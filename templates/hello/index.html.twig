{% extends 'base.html.twig' %}

{% block pageTitle %}
    <h3>Home</h3>
{% endblock %}

{% block body %}
    {% if messages|length > 0 %}
        {% for key, message in messages|slice(0, limit) %}
            <div>
                {{ include('/hello/_message.html.twig', {key: key, message: message}) }}
            </div>
        {% endfor %}
    {% else %}
        There's nothing here!
    {% endif %}

{% endblock %}